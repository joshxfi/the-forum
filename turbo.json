{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "dependsOn": ["^build"]
    },
    "generate": { "outputs": ["dist/**"] },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
